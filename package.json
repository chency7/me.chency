{
  "name": "chency.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:standalone": "next build && npm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "start": "next start -p 3005",
    "start:standalone": "cross-env NODE_ENV=production node .next/standalone/server.js",
    "lint": "next lint",
    "test": "jest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf node_modules .next",
    "clean:all": "rimraf node_modules .next .contentlayer dist build",
    "preinstall": "npx only-allow pnpm",
    "contentlayer": "cross-env NODE_OPTIONS=\"--enable-source-maps\" contentlayer build",
    "clean:content": "rimraf .contentlayer",
    "rebuild:content": "pnpm clean:content && pnpm contentlayer"
  },
  "dependencies": {
