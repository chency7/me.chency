    "fontfaceobserver": "^2.3.0",
    "framer-motion": "^10.16.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.284.0",
    "markdown-wasm": "^1.2.0",
    "next": "^13.5.4",
    "next-contentlayer": "^0.3.4",
    "postcss-import": "^16.1.0",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
